{
	"database": {
		"name": "intoj",
        "user": "root",
        "host": "localhost",
        "pass": ""
	},
	"redis": {
		"host": "localhost",
        "port": "6379",
        "pass": ""
	},
	"compilation": {
		"max_time": 10000,
		"max_memory": 1024,
		"max_output": 40960
	},
	"languages": {
		"cpp": {
			"extension": "cpp",
			"compile_command": "g++ {source} -o {exe} -O2 -std=c++03 -fmax-errors=4 -w -static",
			"run_command": "{exe}"
		},
		"cpp11": {
			"extension": "cpp",
			"compile_command": "g++ {source} -o {exe} -O2 -std=c++11 -fmax-errors=4 -w -static",
			"run_command": "{exe}"
		},
		"cpp17": {
			"extension": "cpp",
			"compile_command": "g++ {source} -o {exe} -O2 -std=c++17 -fmax-errors=4 -w -static",
			"run_command": "{exe}"
		}
	},
	"previews": {
		"input": 256,
		"output": 256,
		"stdout": 256,
		"stderr": 256,
		"judger_message": 256
	},
	"running": {
		"max_output": 64
	},
	"judging": {
		"max_time": 10000,
		"max_memory": 1024
	},
	"custom_test": {
		"max_time": 4000,
		"max_memory": 1024,
		"max_output_show": 256
	}
}
